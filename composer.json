{
  "name": "goetas-webservices/wsdl2php",
  "authors": [
    {
      "name": "Asmir Mustafic",
      "email": "goetas@gmail.com"
    }
  ],
  "require": {
    "php": "^7.1",
    "goetas-webservices/soap-reader": "^0.3.1",
    "goetas-webservices/wsdl-reader": "^0.3",
    "goetas-webservices/xsd2php": "^0.2.3|^0.3.0|^0.4.0",
    "symfony/dependency-injection": "^2.2|^3.0|^4.0",
    "symfony/config": "^2.2|^3.0|^4.0",
    "symfony/yaml": "^2.1|^3.0|^4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0|^8.0",
    "jms/serializer": "^1.2|^2.0|^3.0",
    "goetas-webservices/xsd2php-runtime": "^0.2.2"
  },
  "autoload": {
    "psr-4": {
      "GoetasWebservices\\WsdlToPhp\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "GoetasWebservices\\WsdlToPhp\\Tests\\": "tests",
      "GoetasWebservices\\Xsd\\XsdToPhp\\Tests\\": "vendor/goetas-webservices/xsd2php/tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.3-dev"
    }
  },
  "bin": [
    "bin/wsdl2php"
  ]
}
